{
  "name": "simulated-multitree",
  "version": "1.0.0",
  "description": "An in-memory simulated MultiTree for testing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "ava -s test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitwebs/simulated-multitree.git"
  },
  "author": "Paul Frazee <pfrazee@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitwebs/simulated-multitree/issues"
  },
  "homepage": "https://github.com/bitwebs/simulated-multitree#readme",
  "devDependencies": {
    "@types/concat-stream": "^1.6.1",
    "ava": "^3.15.0",
    "concat-stream": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
